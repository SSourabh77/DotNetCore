@model List<Student>
@{
    ViewData["Title"] = "About";
}

<h1>About</h1>

<table border="1">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Gender</td>
        <td>Class</td>
    </tr>
    <tbody>
        @{
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Gender</td>
                    <td>@item.Class</td>
                
                </tr>
            }
        }


    </tbody>
</table>